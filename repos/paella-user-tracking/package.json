{
	"name": "@asicupv/paella-user-tracking",
	"version": "2.3.3",
	"exports": {
		".": "./dist/paella-user-tracking.js"
	},
	"description": "User analytics and tracking plugins for Paella Player",
	"main": "./dist/paella-user-tracking.js",
	"publishConfig": {
		"access": "public"
	},
	"type": "module",
	"module": "dist/paella-user-tracking.js",
	"files": [
		"dist/paella-user-tracking.js",
		"dist/paella-user-tracking.js.map",
		"dist/paella-user-tracking.d.ts"
	],
	"scripts": {
		"dev": "vite build --watch",
		"build": "vite build --emptyOutDir && npm run bundle:types",
		"bundle:types": "dts-bundle-generator -o dist/paella-user-tracking.d.ts dist/index.d.ts --external-inlines=@asicupv --no-check",
		"types": "tsc -p tsconfig.debug.json"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/polimediaupv/paella-player.git"
	},
	"author": "Fernando Serrano Carpena <ferserc1@gmail.com>",
	"license": "SEE LICENSE IN license.txt",
	"bugs": {
		"url": "https://github.com/polimediaupv/paella-user-tracking"
	},
	"homepage": "https://paellaplayer.upv.es",
	"devDependencies": {
		"vite": "^7.1.11",
		"vite-plugin-dts": "^4.5.4",
		"vite-plugin-static-copy": "^3.0.0"
	},
	"peerDependencies": {
		"@asicupv/paella-core": "^2.9.5"
	}
}
