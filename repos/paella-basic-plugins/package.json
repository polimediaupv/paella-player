{
	"name": "@asicupv/paella-basic-plugins",
	"version": "2.6.0",
	"exports": {
		".": "./dist/paella-basic-plugins.js",
		"./src/": "./src/",
		"./paella-basic-plugins.css": "./dist/paella-basic-plugins.css"
	},
	"description": "Basic plugins for Paella Player",
	"main": "./dist/paella-basic-plugins.js",
	"publishConfig": {
		"access": "public"
	},
	"type": "module",
	"types": "dist/paella-basic-plugins.d.ts",
	"module": "./dist/paella-basic-plugins.js",
	"files": [
		"dist/paella-basic-plugins.css",
		"dist/paella-basic-plugins.js",
		"dist/paella-basic-plugins.js.map",
		"dist/paella-basic-plugins.d.ts"
	],
	"scripts": {
		"dev": "vite build --watch",
		"build": "vite build --emptyOutDir && npm run bundle:types",
		"bundle:types": "dts-bundle-generator -o dist/paella-basic-plugins.d.ts dist/index.d.ts --external-inlines=@asicupv --no-check",
		"types": "tsc -p tsconfig.debug.json",
		"i18n:check": "i18n-check -s es-ES -l ./src/i18n"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/polimediaupv/paella-basic-plugins.git"
	},
	"author": "Fernando Serrano Carpena <ferserc1@gmail.com>",
	"license": "SEE LICENSE IN license.txt",
	"bugs": {
		"url": "https://github.com/polimediaupv/paella-player"
	},
	"homepage": "https://github.com/polimediaupv/paella-player#readme",
	"devDependencies": {
		"vite": "^7.1.11",
		"vite-plugin-dts": "^4.5.4",
		"vite-plugin-static-copy": "^3.0.0"
	},
	"peerDependencies": {
		"@asicupv/paella-core": "^2.11.4"
	}
}
